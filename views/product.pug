extends layout
block content
  h1= title
  p Welcome to the #{title} page.
  -
    let results = [
      { productName: "Iphone", price: 10.99, description: "OS-IOS" },
      { productName: "MAC Book", price: 19.99, description: "OS-MAC OS" },
      { productName: "Google pixel", price: 25.49, description: "OS-ANDROID" }
    ]
  div.container-fluid
    div.row
      each item in results
        div.col-sm-2
        h3 Product Name: #{item.productName}
        p Price: $#{item.price}
        p Description: #{item.description}

